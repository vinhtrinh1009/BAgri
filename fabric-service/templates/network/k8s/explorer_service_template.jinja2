apiVersion: v1
kind: Service
metadata:
  name: explorer
  labels:
    name: explorer
spec:
  type: NodePort
  selector:
    app: explorer
  ports:
  - protocol: TCP
    port: 8080
    targetPort: 8080
---
apiVersion: v1
kind: Service
metadata:
  name: explorer-db
  labels:
    name: explorer-db
spec:
  selector:
    app: explorer-db
  ports:
  - protocol: TCP
    port: 5432
    targetPort: 5432
